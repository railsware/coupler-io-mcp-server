{
  "name": "coupler-io-mcp-server",
  "version": "0.0.1",
  "main": "index.js",
  "type": "module",
  "homepage": "https://coupler.io",
  "scripts": {
    "start": "bun src/index.ts",
    "inspect:bun": "bun mcp-inspector --config config/mcp-inspector/development.json --server coupler_bun",
    "inspect:docker": "bun mcp-inspector --config config/mcp-inspector/development.json --server coupler_docker",
    "lint": "eslint src/ --ext .ts"
  },
  "description": "Coupler.io MCP server",
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.11.0",
    "lodash": "4.17.21",
    "pino": "9.6.0",
    "url-template": "3.1.1",
    "znv": "0.5.0",
    "zod": "3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "9.25.1",
    "@modelcontextprotocol/inspector": "0.11.0",
    "@types/bun": "1.2.11",
    "@types/lodash": "4.17.16",
    "@types/node": "22.15.3",
    "eslint": "9.25.1",
    "lefthook": "1.11.12",
    "pino-pretty": "13.0.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.31.1"
  }
