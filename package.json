{
  "name": "coupler-io-mcp-server",
  "version": "0.0.1",
  "main": "index.js",
  "type": "module",
  "homepage": "https://coupler.io",
  "bin": {
    "coupler-io-mcp-server": "./dist/index.js"
  },
  "scripts": {
    "start": "bun src/index.ts",
    "inspect:bun": "bun mcp-inspector --config config/mcp_server/development.json --server coupler_bun",
    "inspect:docker": "bun mcp-inspector --config config/mcp_server/development.json --server coupler_docker",
    "lint": "eslint src/ --ext .ts"
  },
  "description": "Coupler.io MCP server",
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.10.2",
    "lodash": "4.17.21",
    "pino": "9.6.0",
    "url-template": "3.1.1",
    "znv": "0.5.0",
    "zod": "3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "9.25.1",
    "@modelcontextprotocol/inspector": "0.10.2",
    "@types/bun": "1.2.10",
    "@types/lodash": "4.17.16",
    "@types/node": "22.15.2",
    "eslint": "9.24.0",
    "lefthook": "1.11.9",
    "pino-pretty": "13.0.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.31.0"
  }
}
