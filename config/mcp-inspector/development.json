{
  "mcpServers": {
    "coupler_docker": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "--env-file",
        ".env.local",
        "--add-host",
        "storage.test=host-gateway",
        "--add-host",
        "lvh.me=host-gateway",
        "coupler-io-mcp-server-development"
      ]
    },
    "coupler_node": {
      "command": "npm",
      "args": [
        "run",
        "dev"
      ],
      "env": {
        "TRANSPORT": "stdio"
      }
    },
    "coupler_http": {
      "type": "streamable-http",
      "url": "http://localhost:3001/mcp"
    }
  }
}
